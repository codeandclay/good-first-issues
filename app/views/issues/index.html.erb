<div class="tags-wrapper">
  <div class="content">
    <div class="tags">
      <%= link_to 'See all tags', tags_path, class: 'tags-link' %>
    </div>
  </div>
</div>

<div class="content issues">
  <h1><%= @issues.title %> <span class="meta">Updated <%= time_ago_in_words(@issues.updated_at) %> ago</span></h1>

  <%= will_paginate @issues, class: 'pagination top-pagination' %>
  <%= render @issues %>
  <%= will_paginate @issues, class: 'pagination bottom-pagination' %>
</div>
